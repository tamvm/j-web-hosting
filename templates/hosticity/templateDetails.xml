			<option value="dark">dark</option>
		</param>
		<param name="xml_s5_s5box_effect" type="list" default="drop" label="S5 Box Drop or Fade" description="If you choose to use the S5 Box by publishing any module to the toolbar position would you like the box to drop in or fade in?">
			<option value="drop">drop</option>
			<option value="fade">fade</option>
		</param>
		<param name="xml_s5_phone_number" type="text" default="1-800-800-0000" label="Phone Number" description="If you wish to have your phone number appear on the header enter it below. If you do not wish to have it appear enter the word none." />
		<param name="xml_s5_body_width" type="text" default="950" label="Body Width" description="What width would you like for the website?" />
		<param name="xml_s5_right_width" type="text" default="190" label="Right Width" description="Enter the width of the main right column in pixels." />
		<param name="xml_s5_left_width" type="text" default="220" label="Left Width" description="Enter the width of the left position in pixels." />
		<param name="xml_s5_menu" type="list" default="1" label="Menu Type" description="This template comes with 2 built in drop down menu options, the S5 No-MooMenu and the standard suckerfish menu.">
			<option value="1">S5 No-MooMenu</option>
			<option value="2">Suckerfish</option>
		</param>
		<param name="xml_s5_no_moo_type" type="list" default="fade" label="S5 No-MooMenu Fade or Drop?" description="If you chose to use the S5 No-MooMenu you may now choose to have the menu drop in from the corner or fade in. Please note that if you have a lot of animation on your page the drop in menu can run slowly because of issues with javascript.">
			<option value="fade">fade</option>
			<option value="drop">drop</option>
		</param>
		<param name="xml_s5_tooltips" type="list" default="yes" label="Would you like to enable tooltips?">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>
		<param name="spacer4" type="spacer" default="Enter the title of each tab below that you are using. Remember to also publish a module to the corresponding tab number or the tab will not show. Use the module manager to change which tabs show on which pages. Please visit our demo page for a complete tutorial at demo.shape5.com" />
		<param name="xml_s5_tab_show_pos" type="list" default="1" label="S5 Tab Show Position" description="">
			<option value="1">Above Main Body</option>
