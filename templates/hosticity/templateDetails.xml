	<params>
		<param name="spacer1" type="spacer" default="Make sure to hover your mouse over each title in this column for a full description of what each field does." />
		<param name="xml_menuname" type="text" default="mainmenu" label="Menu Name" description="What menu would you like to use for the default default menu?" />
        <param name="xml_s5_style" type="list" default="style1" label="Color Style" description="Enter the style number you wish to use.">
			<option value="style1">style1</option>
