<?xml version="1.0" encoding="iso-8859-1"?>
<install type="module" version="1.5.0">
	<name>S5 Domain Check</name>
	<author>Shape5.com</author>
	<creationDate>August 2008</creationDate>
	<license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>
	<authorEmail>contact@shape5.com</authorEmail>
	<authorUrl>www.shape5.com</authorUrl>
	<version>1.0.0</version>
	<description>This module will display available domains.</description>
	<files>
		<filename module="mod_s5_domain_check">mod_s5_domain_check.php</filename> 
	</files>
	<params>
	<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="A suffix to be applied to the css class of the module (table.moduletable), this allows individual module styling" />
	<param name="pretext" type="textarea" cols="40" rows="5" default="" label="Pre-Text" description="Enter any pre-text message." />
	<param name="label" type="text" default="Domain Name" label="Label" description="" />
	<param name="buttontext" type="text" default="Check Domain" label="Button Text" description="" />
	<param name="checkall" type="text" default="Check All Domain Types" label="Check All Text" description="" />
	<param name="resulttext" type="text" default="Result" label="Result" description="" />
	</params>
</install>
